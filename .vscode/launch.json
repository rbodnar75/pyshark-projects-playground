{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "python",
            "request": "launch",
            "name": "Launch Pyshark Script",
            "program": "${workspaceFolder}/${input:scriptName}.py",
            "preLaunchTask": "install pyshark"
        }
    ],
    "inputs": [
        {
            "type": "pickString",
            "id": "scriptName",
            "description": "Select the Python script to launch",
            "options": [
                "Sample Files/dhcpfilterpacketcapsave",
                "Sample Files/packetcapturewrite-html",
                "Sample Files/packetcapturewrite",
                "test"
            ]
        }
    ]
}